  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#Challenge">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Challenge</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Challenging an opponent.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://smartweb.semanticweb.org/ontology/sportevent#Play"/>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#HalfTime">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The intermission between the 2 periods or halves of a game.</rdfs:comment>
