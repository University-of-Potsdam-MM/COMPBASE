  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#AssociationFootball">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >AssociationFootball</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The traditional term for the game in England to distinguish it from rugby football abbreviated assoc. Over time the prefix was dropped and in English custom, "er" added to the end to form the word "soccer".</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#FootballTerm"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#AllroundPlayer">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >AllroundPlayer</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#PropertyFootballPlayer"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
