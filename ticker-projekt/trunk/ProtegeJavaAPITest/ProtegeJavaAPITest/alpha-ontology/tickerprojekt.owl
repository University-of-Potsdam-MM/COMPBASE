  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#_4-3-3Formation">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#CompleteFootballFormation"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A formation that consists of 4 defenders, 3 midfielders and 3 forwards.</rdfs:comment>
