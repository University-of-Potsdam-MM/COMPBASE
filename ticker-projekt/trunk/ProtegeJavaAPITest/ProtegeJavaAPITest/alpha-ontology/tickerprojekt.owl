      <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#DefensiveChallenge"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Block</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >When a defensive player, instead of going after the ball, uses his body to prevent an offensive player from playing it.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#Diving">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#Foul"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Diving</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >When a player pretends that his direct opponent has made on him and falls down without being fouled.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#AmericanSoccerLeague">
