    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >TalliedShoot</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A shoot that ends to scoring a goal.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#FootballRefereeing">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#RefereeAction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#Volley">
    <rdfs:subClassOf rdf:resource="http://smartweb.semanticweb.org/ontology/sportevent#FootKick"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Volley</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Kicking the ball out of the air before it hits the ground.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#Derby">
