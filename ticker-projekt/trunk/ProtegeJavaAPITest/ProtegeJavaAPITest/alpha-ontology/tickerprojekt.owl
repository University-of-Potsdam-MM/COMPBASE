    >American soccer league.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#NationalFootballLeague"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#PositionalFootball">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#FootballSystemOfPlaying"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A kind of system of playing.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >PositionalFootball</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#CenterSpot">
