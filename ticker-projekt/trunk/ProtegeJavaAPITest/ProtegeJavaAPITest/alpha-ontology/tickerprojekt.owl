    >HalfTime</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://smartweb.semanticweb.org/ontology/sportevent#SportTimePeriod"/>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#HighPass">
