    >NASL</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://smartweb.semanticweb.org/ontology/sportevent#AmericanSoccerLeague"/>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#Rival">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The contestant you hope to defeat.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#Contestant"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
