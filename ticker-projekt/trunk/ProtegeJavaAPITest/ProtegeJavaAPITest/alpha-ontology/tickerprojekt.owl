      <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#FootballTeam"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#NationalTeam"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A certain division of a national football team -including Squad players and OtherPeople- which take part in a tournament.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#CreatingSpace">
    <rdfs:subClassOf rdf:resource="http://smartweb.semanticweb.org/ontology/sportevent#BodyAction"/>
