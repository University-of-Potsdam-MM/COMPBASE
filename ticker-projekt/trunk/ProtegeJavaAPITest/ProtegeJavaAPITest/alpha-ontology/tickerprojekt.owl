    <rdfs:subClassOf>
      <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#MatchType"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Derby</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#LeagueFootballMatch">
