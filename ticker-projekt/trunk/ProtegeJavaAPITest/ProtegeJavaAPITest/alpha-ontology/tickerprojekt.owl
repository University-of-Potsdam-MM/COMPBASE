    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Line</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://smartweb.semanticweb.org/ontology/sportevent#TalliedShoot">
